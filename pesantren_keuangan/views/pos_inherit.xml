<odoo>
    <data>
        <record id="view_product_combo_form_inherit" model="ir.ui.view">
            <field name="name">product.combo.form.inherit</field>
            <field name="model">product.combo</field>
            <field name="inherit_id" ref="product.product_combo_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='base_price']" position="attributes">
                    <attribute name="string">Harga Combo</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
