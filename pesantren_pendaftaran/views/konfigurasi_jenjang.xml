<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="cdn_jenjang_pengaturan_action" model="ir.actions.act_window">
            <field name="name">Jenjang Pendidikan</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ubig.pendidikan</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="pesantren_list_keonfigurasi_jenjang_action" model="ir.actions.act_window.view">
            <field name="sequence" eval="1"/>
            <field name="view_mode">list</field>
            <field name="view_id" ref="view_ubig_pendidikan_list"/>
            <field name="act_window_id" ref="cdn_jenjang_pengaturan_action"/>
        </record> -

         <record id="pesantren_form_keonfigurasi_jenjang_action" model="ir.actions.act_window.view">
            <field name="sequence" eval="2"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_ubig_pendidikan_form"/>
            <field name="act_window_id" ref="cdn_jenjang_pengaturan_action"/>
        </record> 

         <!-- <menuitem id="cdn_pengaturan_menu_act" name="Jenjang Pendidikan" parent="pesantren_base.pengaturan_umum_menu_categ" action="cdn_jenjang_pengaturan_action" sequence="50"/> -->
    </data>
</odoo>